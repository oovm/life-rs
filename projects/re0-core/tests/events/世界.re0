
/// 你生病了
事件 生病 {
    分支: {
        10: {
    触发: 体 > 6
    效果: [
        得分 += 2
        体质 -= 1
        财富 -= 1
        触发(重病)
    ]
}
    }
}

/// 你得了重病
事件 重病 {
    触发: 体 > 6
    效果: [
        得分 += 2
        体质 -= 1
        财富 -= 1
    ]
    分支: [
       {
           触发: 体 > 6
           效果: [
               得分 += 2
               体质 -= 1
               财富 -= 1
               触发(重病)
           ]
       }
    ]
}